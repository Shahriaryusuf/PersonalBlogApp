<%- include("partials/header") -%>


    
<h1>Compose</h1>

    <form action="/compose" method="post">
        <div class="form-group">
            <h3>Title</h3>
            <input class="form-control" type="text" name="newtitle" autocomplete="off">
        </div>
        <div class="form-group">
            <h3>Post</h3>
            <textarea class="form-control" id="msgArea" name="newpost" cols="60" rows="10"></textarea>
        
        </div>
    
    
    <button class="btn btn-primary" type="submit" name="publishBtn">Publish</button>
    
    </form>
    


<%- include("partials/footer") -%>